def add(x,y):
  answer_1=x+y
  return answer_1
def subtract(x,y):
  answer_2=x-y
  return answer_2
def multiply(x,y):
  answer_3=x*y
  return answer_3
def divide(x,y):
  answer_4=x/y
  return answer_4
def exp(x,y):
  answer_5=x**y
  return answer_5
print(add(1,2))
print(subtract(2,1))
print(multiply(9,8))
print(divide(9,3))
print(exp(3,2))
